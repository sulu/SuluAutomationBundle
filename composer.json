{
    "name": "sulu/automation-bundle",
    "type": "sulu-bundle",
    "description": "The Sulu Bundle which provides automation functionality",
    "license": "MIT",
    "keywords": ["sulu", "automation"],
    "authors": [
        {
            "name": "Sulu Community",
            "homepage": "https://github.com/sulu/SuluAutomationBundle/contributors"
        }
    ],
    "require": {
        "php": "^7.1",
        "friendsofsymfony/rest-bundle": "^2.0",
        "symfony/config": "^3.4",
        "symfony/dependency-injection": "^3.4",
        "symfony/http-foundation": "^3.4",
        "symfony/http-kernel": "^3.4",
        "symfony/force-lowest": "^3.4 || ^4.0",
        "sulu/sulu": "^2.0",
        "php-task/task-bundle": "^2.0",
        "willdurand/hateoas-bundle": "^1.2",
        "jms/serializer-bundle": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.5",
        "symfony/stopwatch": "^3.4",
        "symfony/browser-kit": "^3.4",
        "symfony/monolog-bundle": "^3.1",
        "jackalope/jackalope-doctrine-dbal": "~1.2",
        "zendframework/zendsearch": "2.*@RC"
    },
    "autoload": {
        "psr-4": {
            "Sulu\\Bundle\\AutomationBundle\\": ""
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-develop": "2.0-dev"
        }
    }
}
